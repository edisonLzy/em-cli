function createFile(file, callback) {
  function makeFile() {
    fs$9.writeFile(file, '', err => {
      if (err) return callback(err);
      callback();
    });
  }

  fs$9.stat(file, (err, stats) => {
    // eslint-disable-line handle-callback-err
    if (!err && stats.isFile()) return callback();
    const dir = path$b.dirname(file);
    fs$9.stat(dir, (err, stats) => {
      if (err) {
        // if the directory doesn't exist, make it
        if (err.code === 'ENOENT') {
          return mkdir$2.mkdirs(dir, err => {
            if (err) return callback(err);
            makeFile();
          });
        }

        return callback(err);
      }

      if (stats.isDirectory()) makeFile();else {
        // parent is not a directory
        // This is just to cause an internal ENOTDIR error to be thrown
        fs$9.readdir(dir, err => {
          if (err) return callback(err);
        });
      }
    });
  });
}

function createFileSync(file) {
  let stats;

  try {
    stats = fs$9.statSync(file);
  } catch {}

  if (stats && stats.isFile()) return;
  const dir = path$b.dirname(file);

  try {
    if (!fs$9.statSync(dir).isDirectory()) {
      // parent is not a directory
      // This is just to cause an internal ENOTDIR error to be thrown
      fs$9.readdirSync(dir);
    }
  } catch (err) {
    // If the stat call above failed because the directory doesn't exist, create it
    if (err && err.code === 'ENOENT') mkdir$2.mkdirsSync(dir);else throw err;
  }

  fs$9.writeFileSync(file, '');
}


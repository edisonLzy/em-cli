import { VFile } from 'vfile';
import { FileManagerOptions, FileOptions } from '../types';
export declare class FileManager {
    private stats;
    private _files;
    private op;
    constructor(op?: FileManagerOptions);
    private getLoggerConfig;
    private adaptorAddArgs;
    private _addFile;
    private processSingleOutput;
    private processBatchOutput;
    private processOutput;
    outFile: (dest: string) => Promise<void>;
    addFile: (file: FileOptions | FileOptions[]) => void;
    clear: () => void;
    get files(): VFile[];
    get total(): number;
}

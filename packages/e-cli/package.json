{
  "name": "em-cli",
  "version": "1.0.0",
  "description": "make cli easier",
  "main": "lib/index.js",
  "bin": {
    "em-cli": "./bin/em-cli"
  },
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm run clean && tsc --watch",
    "build": "npm run clean && tsc && npm run move",
    "dev": "ts-node lib/index.ts",
    "lint": "npm-run-all lint:*",
    "move": "npm-run-all move:*",
    "move:taro": "cpr lib/commander/add-taro/template dist/commander/add-taro/template",
    "lint:ts": "eslint --fix"
  },
  "author": "evan",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@babel/plugin-transform-react-jsx": "^7.13.12",
    "@babel/plugin-transform-react-jsx-compat": "^7.12.13",
    "@babel/plugin-transform-react-jsx-source": "^7.12.13",
    "@babel/plugin-transform-typescript": "^7.13.0",
    "@babel/preset-react": "^7.13.13",
    "@types/babel-types": "^7.0.9",
    "@types/babel__core": "^7.1.14",
    "@types/ejs": "^3.0.6",
    "@types/eslint": "^7.2.8",
    "@types/fs-extra": "^9.0.9",
    "@types/node": "^14.14.34",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "babel-types": "^6.26.0",
    "cpr": "^3.0.1",
    "ejs": "^3.1.6",
    "eslint": "^7.22.0",
    "fs-extra": "^9.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "commander": "^7.1.0",
    "enquirer": "^2.3.6",
    "log-symbols": "^4.1.0",
    "ora": "^5.4.0"
  }
}

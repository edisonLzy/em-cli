{
  "packages": ["packages/*"],
  "version": "independent",
  "ignoreChanges": ["**/__tests__/**", "**/*.md", "**/package-log.json"],
  "command": {
    "publish": {
      "registry": "https://registry.npmjs.org/",
      "conventionalCommits": false,
      "message": "chore(release): publish %v"
    }
  }
}
